
@{
    ViewBag.Title = "Finders";
}

<h2>Finders</h2>
<h5>Canopy can find fields by id, label, css, xpath, jQuery, text, or value.</h5>
<form id="form1" method="POST">
    <div class="main-section">
        <div class="fitem">
            <label for="firstName">First name:</label><input type="text" id="firstName" />
        </div>
        <div class="fitem">
            <label for="lastName">Last name:</label><input type="text" id="lastName" />
        </div>
        <div class="fitem">
            <label for="address1">Address:</label><input type="text" id="address1" class="address1"/>
        </div>
        <div class="fitem">
            <label for="address2">Address (second line):</label><input type="text" id="address2" />
        </div>
        <div class="fitem">
            <label for="city">City:</label><input type="text" id="city" class="city"/>
        </div>
        <div class="fitem">
            <label for="state">State:</label><input type="text" id="state" />
        </div>
        <div class="fitem">
            <label for="zip" class="zip-label">Zip:</label><input type="text" id="zip" />
        </div>
        <div class="fitem">
            <label></label><input type="button" id="continue" value="Continue" />
        </div>
    </div>
</form>
@section Scripts{
    <script>
        $(document).ready(function() {
            $("#continue").click(function () {
                location.href = "http://localhost:8080/Home/Index";
            });
        });
    </script>
}

    
